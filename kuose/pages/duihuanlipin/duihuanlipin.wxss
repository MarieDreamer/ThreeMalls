/* pages/duihuanlipin/duihuanlipin.wxss */
.page{
  background-color:#FFFAFA;
}
.duihuan-img{
  width: 100%;
  height: 350rpx;
}
.li{
  width: 100%;
  height: 80rpx;
  border-bottom: 1px #F0EFF5 solid;
  margin-left:20rpx;
  background-color: white;
}
.li-text{
  float: left;
  width: 17%;
  text-align: left;
  line-height: 80rpx;
  font-size: 30rpx;
}
.input{
  float: left;
  width: 80%;
  font-size: 30rpx;
  margin-top: 15rpx;
  margin-left: 10rpx;
}
.save{
  width: 90%;
  height: 80rpx;
  margin-left: 5%;
  text-align: center;
  background-color: red;
  color: white;
  line-height: 80rpx;  
  border-radius: 2%;
  margin-top: 50rpx;
}
.dibu {
  position: fixed;
  bottom: 0rpx;
  background-color: #FFFAFA;
  width:100%;
  height:150rpx;
}
.dibu-text1{
  font-size: 25rpx;
  font-weight: bold;
  text-align: center;
  padding-top: 30rpx;
}
.dibu-text2{
  font-size:25rpx;
  text-align: center;
  padding-top: 20rpx;
  color: #B0C4DE;
}
