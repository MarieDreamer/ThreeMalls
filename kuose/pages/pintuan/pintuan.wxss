/* pages/pintuan/pintuan.wxss */
.youhuiquan{
  width: 700rpx;
  margin: 25rpx;
  height: 180rpx;
  background-color: red;
  border-radius: 2%;
  display: flex;
  align-items: center;
  justify-content: center;
}
.youhuiquan-white{
  color: red;
  background-color: white;
  height: 150rpx;
  width: 80%;
  display: flex;
  align-items: center;
}
.xuxian{
  font-size: 60rpx;
  padding: 0 40rpx;
  height: 100rpx;
  line-height:100rpx;
  display:flex;
  border-right: 1px dashed red;
}
.renminbi{
  font-size: 30rpx;
}
.renminbi-num{
  float:left;
  height: 80rpx;
  font-size: 65rpx;
  line-height: 80rpx;
}
.tiaojian{
  height: 100rpx;
  padding-left: 40rpx;
  font-size: 30rpx;
  line-height:50rpx;
}
.tiaojian-name{
  color: red;
  font-size: 25rpx;

}
.tiaojian-num{
  color: red;
  font-size: 25rpx;
}
.lingqu{
  width: 50rpx;
  font-size: 30rpx;
  color:white;
  margin-left: 30rpx;
}
.baobei{
  width: 700rpx;
  padding: 25rpx;
  height: 200rpx;
  border-bottom: 1rpx #F0EFF5 solid;
}
.baobei-image{
  float: left;
  width: 200rpx;
  height: 200rpx;
}
.baobei-name{
  font-size: 30rpx;
}
.baobei-wenan{
  width: 480rpx;
  height: 200rpx;
  float: right;
  display: flex;
  flex-direction: column;
  align-items: flex-start;
  justify-content: space-between;
}
.baobei-pintuanjia{
  color: white;
  background-color: red;
  font-size: 25rpx;
  text-align: center;
  border-radius: 20rpx 0 0 20rpx;
  height: 40rpx;
  line-height: 40rpx;
  padding: 0 15rpx;
}
.baobei-renminbi{
  font-size: 30rpx;
  color: red;
}
.baobei-num{
  font-size: 30rpx;
  color:red;
}
.baobei-danmaijia{
  font-size: 30rpx;
  float: left;
}
.baobei-qukaituan{
  text-align: center;
  float: right;
  color: red;
  font-size: 30rpx;
  border:1rpx red solid;
  border-radius: 10rpx;
  padding: 0 20rpx;
}
.rule{
  width: 647rpx;
  margin: 25rpx;
  border: 3rpx red dashed;
  text-align: center;
  padding: 25rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.rule text{
  font-size: 35rpx;
  color: gray
}
.rule-title{
  width: 200rpx;
  height: 50rpx;
  background-color: red;
  color: white;
}
.rule-rule{
  color: red;
}
.mingdan{
  display: flex;
  flex-direction: column;
  align-items: center;
  text-align: center;
  margin: 40rpx 0;
}
.mingdan-title{
  color:white;
  background-color: red;
  font-size: 60rpx;
  width: 500rpx;
}
.mingdanimg{
  height: 600rpx;
}
.mingdanimg-kuang1{
  float: left;
  width: 30%;
  padding-top: 50rpx;
  padding-left: 40rpx;
}
.mingdanimg-kuang2{
  float: left;
   padding-top: 50rpx;
  width: 50%;
}
.mingdanimg-hd{
  width: 200rpx;
  height: 200rpx;
}
.mingdanimg-name{
  width: 450rpx;
}
.dibu {
  background-color: #FFFAFA;
  width:100%;
  height:150rpx;
}
.dibu-text1{
  font-size: 25rpx;
  font-weight: bold;
  text-align: center;
  padding-top: 30rpx;
}
.dibu-text2{
  font-size:25rpx;
  text-align: center;
  padding-top: 20rpx;
  color: #B0C4DE;
}
